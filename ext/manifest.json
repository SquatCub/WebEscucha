{
    "manifest_version": 2,
    "name": "WebEscucha",
    "description": "Permite escuchar el contenido principal de la pagina web",
    "version": "1.0",
    "icons": {
        "128": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab"
    ]
}